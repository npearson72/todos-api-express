{
  "name": "backend",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "db:migrate": "knex migrate:latest --knexfile='config/db/knexfile.js'",
    "db:rollback": "knex migrate:down --knexfile='config/db/knexfile.js'",
    "db:create": "node -r dotenv/config lib/scripts/db/createDatabase.js",
    "db:drop": "node -r dotenv/config lib/scripts/db/dropDatabase.js",
    "db:seed": "knex seed:run --knexfile='config/db/knexfile.js'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "joi": "^17.4.0",
    "knex": "^0.21.19",
    "module-alias": "^2.2.2",
    "objection": "^2.2.15",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "_moduleAliases": {
    "@actions": "./actions",
    "@lib": "./lib",
    "@models": "./models"
  }
}
